# Copy to .env and fill in values.
# For local development, run `npx supabase start` and use the credentials it prints,
# or run `npx supabase status` to see them again.

# Local Supabase (defaults when running `supabase start`)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=<from `npx supabase status`>
SUPABASE_SERVICE_ROLE_KEY=<from `npx supabase status`>

# Required: direct Postgres connection for FastAPI
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres
